{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/essential"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        // override/add rules settings here, such as:
        "vue/no-unused-vars": "error",
        "vue/require-prop-types": "error",
        "vue/this-in-template": "error",
        "semi": [2, "always"],
        "quotes": [2, "single"]
    }
}
// https://eslint.org/docs/rules/
